{
  "name": "transcribe-cli",
  "description": "Transcribe an audio file.",
  "version": "0.0.1",
  "author": "Bard Lind",
  "bin": {
    "transcribe": "./bin/run"
  },
  "bugs": "https://github.com/nrkno/transcriber-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/configstore": "^4.0.0",
    "@types/node-fetch": "^2.1.7",
    "@types/open": "^6.0.0",
    "cli-ux": "^5.2.1",
    "configstore": "^4.0.0",
    "firebase": "^5.9.0",
    "node-fetch": "^2.6.7",
    "open": "^6.0.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "prettier": "^1.16.4",
    "ts-node": "^8",
    "tslint": "^5",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/nrkno/transcriber-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "transcribe",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "nrkno/transcriber-cli",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
